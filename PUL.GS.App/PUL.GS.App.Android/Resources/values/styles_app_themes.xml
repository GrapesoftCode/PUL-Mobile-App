<?xml version="1.0" encoding="utf-8" ?>
<resources>

  <!--When you introduce a new theme in Core project's ThemeResources folder. Also create similar copy for android specific styling as below for that theme-->

  <style name="whiteAppTheme" parent="MainBaseTheme">
    <item name="toolbarStyle">@style/whiteThemeToolbarStyle</item>
    <item name="toolbarPopupStyle">@style/lightToolbarPopupTheme</item>
    <item name="colorPrimaryDark">#ffffff</item>
    <item name="android:windowLightStatusBar">true</item>
         <item name="actionOverflowButtonStyle">@style/OverflowButtonStyle</item>
     
  </style>
    <style name="OverflowButtonStyle" parent="@android:style/Widget.ActionButton.Overflow">
    <item name="android:src">@drawable/location</item>
       
  </style>
</resources>